#include <stdio.h>
#include <string.h>
// 숫자1과 숫자0
int main(){
 char alpha[6] = { 'H', 'E', 'L', 'L', 'O', '!'} ;
	char a[6] = { '1', '2', '3', '4', '5', '6'} ;
  char c;
  int i;
	
 scanf("%c", &c);
 for(i=0;alpha[i]!='\0char alpha[6] = { 'H', 'E', 'L', 'L', 'O', '!'} ;
    if(alpha[i] == c){
       printf("%d번째\n", i+1);
       break;
    }
 }
   return 0;
}
